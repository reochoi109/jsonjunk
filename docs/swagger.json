                        }
                    ]
                },
                "language": {
                    "type": "string"
                },
                "title": {
                    "type": "string"
                }
            }
        },
        "model.PasteResponse": {
            "type": "object",
            "required": [
                "language",
                "title"
            ],
            "properties": {
                "content": {
                    "type": "string"
                },
                "created_at": {
                    "type": "string"
                },
                "expires_at": {
                    "type": "string"
                },
                "id": {
                    "type": "string",
