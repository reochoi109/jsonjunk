            "type": "integer",
            "enum": [
                1,
                2,
                3,
                4
            ],
            "x-enum-varnames": [
                "Expire6Hours",
                "Expire12Hours",
                "Expire1Day",
                "Expire7Days"
            ]
        },
        "model.ExpiredTypeResponse": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string"
                },
                "type": {
                    "type": "integer"
                }
            }
        },
        "model.PasteRequest": {
            "type": "object",
            "required": [
                "content",
                "expire",
                "language",
                "title"
            ],
            "properties": {
                "content": {
                    "type": "string"
                },
                "expire": {
                    "enum": [
                        1,
                        2,
                        3,
                        4
                    ],
                    "allOf": [
